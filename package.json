{
  "name": "getfitskg",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "node -e \"if(process.platform!=='win32'){require('child_process').execSync('chmod +x node_modules/.bin/vite',{stdio:'inherit'})}\"",
    "build": "vite build --config vite.production.config.ts",
    "build:mobile": "vite build",
    "build:simple": "vite build --config vite-simple.config.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "cap:sync": "cap sync",
    "cap:sync:android": "cap sync android",
    "cap:sync:ios": "cap sync ios",
    "cap:open:android": "cap open android",
    "cap:open:ios": "cap open ios",
    "cap:run:android": "cap run android",
    "cap:run:ios": "cap run ios",
    "mobile:prepare": "npm run build:mobile && npm run cap:sync",
    "android:build": "npm run build:mobile && npm run cap:sync:android && npm run cap:open:android",
    "ios:build": "npm run build:mobile && npm run cap:sync:ios && npm run cap:open:ios",
    "test:comprehensive": "tsx scripts/runComprehensiveTests.ts",
    "test:massive": "tsx scripts/runMassiveTests.ts",
    "test:massive-simple": "node scripts/runMassiveTestsSimple.cjs",
    "test:simple": "node scripts/basicTestRunner.cjs",
    "test:sql": "echo 'Run database/COMPREHENSIVE_MEMBERSHIP_TEST.sql in your database'",
    "test:all": "npm run test:simple && npm run test:sql",
    "test:all-massive": "npm run test:massive-simple",
    "postinstall": "node -e \"if(process.platform!=='win32'){require('child_process').execSync('chmod -R +x node_modules/.bin',{stdio:'inherit'})}\"",
    "icons:generate": "tsx scripts/generateAppIcons.ts",
    "screenshots:resize": "tsx scripts/resizeAppStoreScreenshots.ts",
    "appstore:check": "tsx scripts/checkAppStoreRequirements.ts",
    "version:show": "tsx scripts/versionManager.ts show",
    "version:bump": "tsx scripts/versionManager.ts bump",
    "version:patch": "tsx scripts/versionManager.ts patch"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/app": "^7.1.0",
    "@capacitor/core": "^7.4.3",
    "@capacitor/ios": "^7.4.3",
    "@capacitor/splash-screen": "^7.0.3",
    "@capacitor/status-bar": "^7.0.3",
    "@emailjs/browser": "^4.4.1",
    "@supabase/supabase-js": "^2.39.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@zxing/browser": "^0.1.5",
    "@zxing/library": "^0.21.3",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "jsonwebtoken": "^9.0.2",
    "jsqr": "^1.4.0",
    "lucide-react": "^0.294.0",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^7.9.4",
    "react-webcam": "^7.2.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.3",
    "@types/node": "^20.19.21",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.5.6",
    "sharp": "^0.34.4",
    "tailwindcss": "^3.4.17",
    "terser": "^5.44.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^5.0.8"
  }
}
