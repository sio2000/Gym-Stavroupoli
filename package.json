{
  "name": "getfitskg",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test:comprehensive": "tsx scripts/runComprehensiveTests.ts",
    "test:massive": "tsx scripts/runMassiveTests.ts",
    "test:massive-simple": "node scripts/runMassiveTestsSimple.cjs",
    "test:simple": "node scripts/basicTestRunner.cjs",
    "test:sql": "echo 'Run database/COMPREHENSIVE_MEMBERSHIP_TEST.sql in your database'",
    "test:all": "npm run test:simple && npm run test:sql",
    "test:all-massive": "npm run test:massive-simple"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "tsx": "^4.20.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
