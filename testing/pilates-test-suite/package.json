    "joi": "^17.11.0",
    "moment-timezone": "^0.5.43",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "artillery": "^2.0.0",
    "autocannon": "^7.14.0",
    "chai": "^4.3.10",
    "joi": "^17.11.0",
    "mocha": "^10.2.0",
    "playwright": "^1.40.0",
    "winston": "^3.11.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
